<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main2Activity">

    <View
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/centro"
        android:background="@color/black"

        />
    <View
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/arribaiz"
        android:background="@color/teal_200"
        app:layout_constraintBottom_toTopOf="@id/centro"
        app:layout_constraintRight_toLeftOf="@+id/centro"

        />
    <View
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/arribader"
        android:background="@color/teal_200"
        app:layout_constraintBottom_toTopOf="@id/centro"
        app:layout_constraintLeft_toRightOf="@+id/centro"

        />
    <View
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/abajoder"
        android:background="@color/teal_200"
        app:layout_constraintTop_toBottomOf="@id/centro"
        app:layout_constraintLeft_toRightOf="@+id/centro"

        />
    <View
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/abajoiz"
        android:background="@color/teal_200"
        app:layout_constraintTop_toBottomOf="@id/centro"
        app:layout_constraintRight_toLeftOf="@+id/centro"

        />
    <View
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/punta"
        android:background="@color/purple_500"
        app:layout_constraintBottom_toTopOf="@id/arribader"
        app:layout_constraintRight_toLeftOf="@+id/arribader"

        />
    <View
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/fulliz"
        android:background="@color/purple_500"
        app:layout_constraintTop_toBottomOf="@id/abajoiz"
        app:layout_constraintRight_toLeftOf="@+id/abajoiz"

        />
    <View
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/fullder"
        android:background="@color/purple_500"
        app:layout_constraintTop_toBottomOf="@id/abajoder"
        app:layout_constraintLeft_toRightOf="@+id/abajoder"

        />
    <View
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/abajo"
        android:background="@color/purple_500"
        app:layout_constraintTop_toBottomOf="@id/abajoder"
        app:layout_constraintRight_toLeftOf="@+id/abajoder"

        />

</androidx.constraintlayout.widget.ConstraintLayout>